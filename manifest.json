{
  "name": "WTR for World of Warships Forum",
  "version": "0.0.5",
  "manifest_version": 2,
  "description": "Colors players' nicknames based on their WRT ranking",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
"content_scripts": [{
    "matches": [
      "*://*.forum.worldofwarships.ru/topic/*"
    ],
    "js": [
      "js/jquery-3.2.1.min.js", 
      "js/analytics.js",
      "js/content.js"
    ],
    "run_at": "document_end",
    "all_frames": true
  }],
  "permissions": [
    "*://*.forum.worldofwarships.ru/topic/*", "*://*.warships.today/*", "https://www.google-analytics.com/*"
  ]
}
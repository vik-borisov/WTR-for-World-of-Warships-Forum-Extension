{
  "name": "WTR for World of Warships Forum",
  "version": "0.0.9",
  "manifest_version": 2,
  "description": "Colors players' nicknames based on their WRT ranking. Displays the name of the clan of the player",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
"settings":{
  "remoteCache": ""
},
"content_scripts": [{
    "matches": [
      "*://*.forum.worldofwarships.ru/topic/*"
    ],
    "js": [
      "js/jquery-3.2.1.min.js", 
      "js/analytics.js",
      "js/playerInfo.js",
      "js/statInfo.js",
      "js/clanInfo.js",
      "js/content.js"
    ],
    "run_at": "document_end",
    "all_frames": true
  }],
  "permissions": [
    "*://*.forum.worldofwarships.ru/topic/*", 
    "*://*.warships.today/*", 
    "https://www.google-analytics.com/*", 
    "https://api.worldofwarships.ru/wows/*",
    "https://www.google.com/analytics/web/inpage/pub/inpage.js*",
    "http://www.google-analytics.com/analytics.js",
    "https://ampcid.google.com/v1/publisher*",
    "https://stats.g.doubleclick.net/r/collect*"
  ]
}